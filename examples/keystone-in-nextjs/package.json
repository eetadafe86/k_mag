    "next": "^13.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "18.7.2",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
